    "version": "0.2.0",
    "configurations": [
        {
            "name": "Mac [Mainnet]",
            "type": "dart",
            "request": "launch",
            "args": [
                "-d",
                "macos",
            ]
        },
        {
            "name": "Mac [Testnet]",
            "type": "dart",
            "request": "launch",
            "args": [
                "-d",
                "macos",
                "--dart-define",
                "TESTNET=true"
            ]
        },
        {
            "name": "Web [Mainnet]",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d",
                "chrome",
                "--web-port",
                "42069",
            ],
        },
        {
            "name": "Web [Testnet]",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d",
                "chrome",
                "--web-port",
                "42069",
                "--dart-define",
                "TESTNET=true"
            ]
        },
    ]
}